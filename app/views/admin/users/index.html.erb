<div class="well users-table">
  <table class="table">
    <h5 style="text-align: center">Users list</h5>
    <thead>
    <tr>
      <th>ID</th>
      <th>Email</th>
      <th>Score</th>
    </tr>
    </thead>
    <tbody>
    <% @users.each do |user| %>
    <tr>
      <td><%= user._id %></td>
      <td><%= user.email %></td>
      <td><%= user.score %></td>
    </tr>
    <% end %>
    </tbody>
  </table>
  <p><%= link_to("Generate Report", users_path(format: :csv), class: "btn btn-success",) %></p>
</div>